<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TinkDemo" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="TinkStyle_Strict" parent="TinkLinkUiStyle">
        <item name="tink_colorBackground">#F7F7F8</item>
        <item name="tink_colorSecondary">#333439</item>
        <item name="tink_colorPrimary">#333439</item>
        <item name="tink_colorOnPrimary">#ffffff</item>
    </style>

    <style name="TinkStyle_ChewingGum" parent="TinkLinkUiStyle">
        <item name="tink_colorBackground">#29262A</item>
        <item name="tink_colorSurface">#343035</item>
        <item name="tink_colorSecondary">#e8a9e0</item>
        <item name="tink_colorPrimary">#e8a9e0</item>
        <item name="tink_colorOnPrimary">#343035</item>
        <item name="tink_dividerColor">#2B2B2B</item>
        <item name="tink_textColorPrimary">#FFFFFF</item>
        <item name="tink_textColorSecondary">#FFFFFF</item>
        <item name="tink_textColorTertiary">#FFFFFF</item>
        <item name="tink_rippleColor">#20FFFFFF</item>
    </style>



</resources>